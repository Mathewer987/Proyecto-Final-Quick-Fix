{
  "version": 2,
  "builds": [
    {
      "src": "InicioSesion.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|ico))",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "InicioSesion.py"
    }
  ],
  "env": {
    "PYTHONPATH": "@",
    "FLASK_ENV": "production"
  }
}