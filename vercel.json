{
  "version": 2,
  "builds": [
    {
      "src": "InicioSesion.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/static/$1"
    },
    {
      "src": "/health",
      "dest": "InicioSesion.py"
    },
    {
      "src": "/(.*)",
      "dest": "InicioSesion.py"
    }
  ],
  "functions": {
    "InicioSesion.py": {
      "maxDuration": 30
    }
  }
}